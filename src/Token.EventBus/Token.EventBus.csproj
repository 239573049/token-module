<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <PackageReleaseNotes>实现本地事件总线
定义指定类型的事件</PackageReleaseNotes>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>Token本地事件总线</Title>
        <PackageProjectUrl>https://github.com/239573049/token-module</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/239573049/token-module</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/239573049/token-module</RepositoryUrl>
        <PackageVersion>1.0.2</PackageVersion>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="6.0.0" />
    </ItemGroup>

</Project>
